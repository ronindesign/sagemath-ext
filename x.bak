{
  "name": "SageMath",
  "version": "0.1",
  "description": "SageMath Extension for Chrome.",
  "browser_action": {
    "default_icon": "icon.png",
	"homepage_url": "https://github.com/ronindesign/sagemath-ext",
	"default_popup": "popup.html",
        "background_page": "background.html",
        "permissions": [
          "http://sagemath.com/*",
          "http://sagemath.org/*"
        ]
  },
  "manifest_version": 2
}